{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "autoincrement",
    "clickhouse",
    "eslintcache",
    "kafkajs",
    "kcmonitor",
    "kincy",
    "nxcache",
    "ttfb",
    "uuidv"
  ],
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[plaintext]": {
    "editor.defaultFormatter": "lkrms.inifmt"
  }
}
