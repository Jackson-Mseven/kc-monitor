{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "autoincrement",
    "clickhouse",
    "commitizen",
    "commitlint",
    "eslintcache",
    "fastify",
    "formbody",
    "kafkajs",
    "kcmonitor",
    "kincy",
    "multer",
    "nxcache",
    "rkey",
    "rollup",
    "sonner",
    "tokei",
    "ttfb",
    "ukey",
    "uuidv"
  ],
  "[dotenv]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[plaintext]": {
    "editor.defaultFormatter": "lkrms.inifmt"
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  }
}
