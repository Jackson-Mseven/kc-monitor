generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int       @id @default(autoincrement())
  username   String
  password   String
  email      String    @unique(map: "email_ukey")
  created_at DateTime? @default(now()) @db.Timestamp(6)
}
